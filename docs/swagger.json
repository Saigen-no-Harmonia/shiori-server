{
    "swagger": "2.0",
    "info": {
        "description": "顔合わせアプリのエンドポイントを提供",
        "title": "Shiori API",
        "contact": {},
        "license": {
            "name": "yagi"
        },
        "version": "1.0"
    },
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "description": "トップ画像と挨拶文を取得。挨拶文は念のため配列だが、要素は１つしか返さない想定。",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "top"
                ],
                "summary": "トップページ取得",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.TopPageResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/gallery": {
            "get": {
                "description": "ギャラリー画像の一覧をlimitとoffsetで取得",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "gallery"
                ],
                "summary": "ギャラリー画像取得",
                "parameters": [
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "取得件数",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 0,
                        "description": "開始位置",
                        "name": "offset",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.GalleryPhotoPageResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/profile": {
            "get": {
                "description": "主催者・参加者・猫プロフィール情報を取得",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "profile"
                ],
                "summary": "プロフィール情報取得",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/dto.ProfilePageResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.GalleryPhotoPageResponse": {
            "type": "object",
            "properties": {
                "galleryPhotos": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.GalleryPhoto"
                    }
                }
            }
        },
        "dto.ProfilePageResponse": {
            "type": "object",
            "properties": {
                "nekoProfiles": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Neko"
                    }
                },
                "participantProfiles": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.ParticipantProfile"
                    }
                },
                "presenterProfiles": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.PresenterProfile"
                    }
                }
            }
        },
        "dto.TopPageResponse": {
            "type": "object",
            "properties": {
                "greetings": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Greeting"
                    }
                },
                "topPhoto": {
                    "$ref": "#/definitions/models.TopPhoto"
                }
            }
        },
        "models.GalleryPhoto": {
            "type": "object",
            "properties": {
                "displayNumber": {
                    "type": "string",
                    "example": "2"
                },
                "id": {
                    "type": "string",
                    "example": "GP001"
                },
                "photoUrl": {
                    "type": "string",
                    "example": "https://s3url"
                },
                "s3ObjectName": {
                    "type": "string",
                    "example": "galleryPhoto.img"
                }
            }
        },
        "models.Greeting": {
            "type": "object",
            "properties": {
                "content": {
                    "type": "string",
                    "example": "こんにちは。たなかです。お忙しい中と存じますが云々。"
                },
                "displayNumber": {
                    "type": "integer",
                    "example": 1
                },
                "id": {
                    "type": "string",
                    "example": "GT001"
                }
            }
        },
        "models.Neko": {
